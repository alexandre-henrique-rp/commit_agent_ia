    "targets": [
      "node18-linux-x64",
      "node18-win-x64",
      "node18-macos-x64"
    ]
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "biome": "^0.3.3",
    "pkg": "^5.8.1"
  }
}
